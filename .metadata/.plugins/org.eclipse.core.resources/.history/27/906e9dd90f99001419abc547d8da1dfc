#ifndef DRIVE_H
#define DRIVE_H
#include <math.h>
#include "WPILib.h"
#include "xBox.hh"

class Drive {
private:
	Victor left(1);
	Victor right(2);
	float speedMult; //speed multiplier
public:
	Drive();
	void move(float leftSpeed, float rightSpeed);
	void remoteDrive(float leftStick, float rightStick, bool boost);
	void stop();
	float getSpeedMult();
};

#endif





